{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:vite\" \"npm run dev:server\" --names \"VITE,SERVER\" --prefix-colors \"cyan,magenta\"",
        "dev:vite": "vite",
        "dev:server": "php artisan serve --port=8001",
        "serve": "php artisan serve --port=8001",
        "build": "vite build",
        "start": "npm run dev"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.2",
        "@tailwindcss/vite": "^4.0.0",
        "alpinejs": "^3.4.2",
        "autoprefixer": "^10.4.2",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^2.0.0",
        "postcss": "^8.4.31",
        "tailwindcss": "^3.1.0",
        "vite": "^7.0.7"
    }
}
